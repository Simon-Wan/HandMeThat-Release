        (cookable ?o - phyobj)
        (dustyable ?o - phyobj)
        (freezable ?o - phyobj)
        (stainable ?o - phyobj)
        (sliceable ?o - phyobj)
        (soakable ?o - phyobj)
        (toggleable ?o - phyobj)

        (valid-clean-pair ?o - phyobj ?o - phyobj)
