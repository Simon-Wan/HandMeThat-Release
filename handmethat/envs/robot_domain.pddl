    (:action robot-move
        :parameters (?r - robot ?from - phyobj ?to - phyobj)
        :precondition (and (robot-at ?r ?from) (not (movable ?from)) (not (movable ?to)))
        :effect (and (robot-at ?r ?to) (not (robot-at ?r ?from)))
    )

    (:action robot-pick-up-at-location
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (hand-empty ?r)
                        (or (and (inside ?obj ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?obj ?loc))
                        (not (movable ?loc)) (movable ?obj) (receptacle ?loc))
        :effect (and (not (hand-empty ?r)) (not (inside ?obj ?loc)) (not (ontop ?obj ?loc)) (holding ?r ?obj))
    )

    (:action robot-pick-up-from-receptacle-at-location
        :parameters (?r - robot ?obj - phyobj ?rec - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (hand-empty ?r)
                        (or (and (inside ?obj ?rec) (or (not (openable ?rec)) (open ?rec))) (ontop ?obj ?rec))
                        (or (and (inside ?rec ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?rec ?loc))
                        (not (movable ?loc)) (movable ?rec) (movable ?obj)
                        (receptacle ?loc) (receptacle ?rec) (not (receptacle ?obj)))
        :effect (and (not (hand-empty ?r)) (not (inside ?obj ?rec)) (not (ontop ?obj ?rec)) (holding ?r ?obj))
    )

    (:action robot-put-inside-location
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (holding ?r ?obj)
                        (not (movable ?loc)) (movable ?obj) (receptacle ?loc)
                        (has-inside ?loc) (or (not (openable ?loc)) (open ?loc)))
        :effect (and (not (holding ?r ?obj)) (inside ?obj ?loc) (hand-empty ?r))
    )

    (:action robot-put-ontop-location
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (holding ?r ?obj)
                        (not (movable ?loc)) (movable ?obj) (receptacle ?loc)
                        (has-ontop ?loc))
        :effect (and (not (holding ?r ?obj)) (ontop ?obj ?loc) (hand-empty ?r))
    )

    (:action robot-put-inside-receptacle-at-location
        :parameters (?r - robot ?obj - phyobj ?rec - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (holding ?r ?obj)
                        (or (and (inside ?rec ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?rec ?loc))
                        (has-inside ?rec) (or (not (openable ?rec)) (open ?rec))
                        (not (movable ?loc)) (movable ?rec) (movable ?obj)
                        (receptacle ?loc) (receptacle ?rec) (not (receptacle ?obj)))
        :effect (and (not (holding ?r ?obj)) (inside ?obj ?rec) (hand-empty ?r))
    )

    (:action robot-put-ontop-receptacle-at-location
        :parameters (?r - robot ?obj - phyobj ?rec - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (holding ?r ?obj)
                        (or (and (inside ?rec ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?rec ?loc))
                        (has-ontop ?rec)
                        (not (movable ?loc)) (movable ?rec) (movable ?obj)
                        (receptacle ?loc) (receptacle ?rec) (not (receptacle ?obj)))
        :effect (and (not (holding ?r ?obj)) (ontop ?obj ?rec) (hand-empty ?r))
    )

    (:action robot-bring-to-human
        :parameters (?r - robot ?obj - phyobj ?human - phyobj)
        :precondition (and (robot-at ?r ?human) (holding ?r ?obj)
                        (movable ?obj) (type-human ?human))
        :effect (and (not (holding ?r ?obj)) (human-holding ?obj) (hand-empty ?r))
    )

    (:action robot-take-from-human
        :parameters (?r - robot ?obj - phyobj ?human - phyobj)
        :precondition (and (robot-at ?r ?human) (hand-empty ?r) (human-holding ?obj)
                        (movable ?obj) (type-human ?human))
        :effect (and (not (hand-empty ?r)) (not (human-holding ?obj)) (holding ?r ?obj))
    )

    (:action robot-open-location
        :parameters (?r - robot ?loc - phyobj)
        :precondition (and (openable ?loc) (robot-at ?r ?loc) (not (open ?loc)))
        :effect (and (open ?loc))
    )

    (:action robot-close-location
        :parameters (?r - robot ?loc - phyobj)
        :precondition (and (openable ?loc) (robot-at ?r ?loc) (open ?loc))
        :effect (and (not (open ?loc)))
    )

    (:action robot-open-receptacle-at-location
        :parameters (?r - robot ?rec - phyobj ?loc - phyobj)
        :precondition (and (openable ?rec) (robot-at ?r ?loc) (not (open ?rec))
                        (or (and (inside ?rec ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?rec ?loc)))
        :effect (and (open ?rec))
    )

    (:action robot-close-receptacle-at-location
        :parameters (?r - robot ?rec - phyobj ?loc - phyobj)
        :precondition (and (openable ?rec) (robot-at ?r ?loc) (open ?rec)
                        (or (and (inside ?rec ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?rec ?loc)))
        :effect (and (not (open ?rec)))
    )

    (:action robot-toggle-on-location
        :parameters (?r - robot ?loc - phyobj)
        :precondition (and (toggleable ?loc) (robot-at ?r ?loc) (not (toggled ?loc)))
        :effect (and (toggled ?loc))
    )

    (:action robot-toggle-off-location
        :parameters (?r - robot ?loc - phyobj)
        :precondition (and (toggleable ?loc) (robot-at ?r ?loc) (toggled ?loc))
        :effect (and (not (toggled ?loc)))
    )

    (:action robot-toggle-on-movable-at-location
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (toggleable ?obj) (robot-at ?r ?loc) (not (toggled ?obj))
                        (or (and (inside ?obj ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?obj ?loc)))
        :effect (and (toggled ?obj))
    )

    (:action robot-toggle-off-movable-at-location
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (toggleable ?obj) (robot-at ?r ?loc) (toggled ?obj)
                        (or (and (inside ?obj ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?obj ?loc)))
        :effect (and (not (toggled ?obj)))
    )

    (:action robot-heat-obj
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (hand-empty ?r) (robot-at ?r ?loc) (cookable ?obj)
                        (or (type-microwave ?loc) (type-stove ?loc) (type-oven ?loc)) (toggled ?loc)
                        (or (inside ?obj ?loc) (ontop ?obj ?loc))
                        (not (receptacle ?obj)) (not (movable ?loc)))
        :effect (and (cooked ?obj) (not (toggled ?loc)) (not (frozen ?obj)))
    )

    (:action robot-cool-obj
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (hand-empty ?r) (robot-at ?r ?loc) (inside ?obj ?loc) (freezable ?obj)
                        (or (type-refrigerator ?loc)) (toggled ?loc) (not (receptacle ?obj)) (not (movable ?loc)))
        :effect (and (frozen ?obj) (not (cooked ?obj)))
    )

    (:action robot-slice-obj
        :parameters (?r - robot ?obj - phyobj ?tool - phyobj ?loc - phyobj)
        :precondition (and (holding ?r ?tool) (type-knife ?tool) (sliceable ?obj)
                        (robot-at ?r ?loc) (ontop ?obj ?loc) (not (receptacle ?obj)) (not (movable ?loc)))
        :effect (and (sliced ?obj))
    )

    (:action robot-soak-obj
        :parameters (?r - robot ?obj - phyobj ?loc - phyobj)
        :precondition (and (robot-at ?r ?loc) (inside ?obj ?loc) (soakable ?obj)
                        (type-sink ?loc) (toggled ?loc) (not (receptacle ?obj)) (not (movable ?loc)))
        :effect (and (soaked ?obj) (not (toggled ?loc)))
    )

    (:action robot-clean-obj-at-location
        :parameters (?r - robot ?obj - phyobj ?tool - phyobj ?loc - phyobj)
        :precondition (and (holding ?r ?tool) (or (dustyable ?obj) (stainable ?obj))
                        (robot-at ?r ?loc) (movable ?obj) (not (movable ?loc))
                        (or (and (inside ?obj ?loc) (or (not (openable ?loc)) (open ?loc))) (ontop ?obj ?loc))
                        (or (not (soakable ?tool)) (soaked ?tool)) (valid-clean-pair ?obj ?tool))
        :effect (and (not (dusty ?obj)) (not (stained ?obj)))
    )

    (:action robot-clean-location
        :parameters (?r - robot ?tool - phyobj ?loc - phyobj)
        :precondition (and (holding ?r ?tool) (or (dustyable ?loc) (stainable ?loc))
                        (robot-at ?r ?loc) (not (movable ?loc))
                        (or (not (soakable ?tool)) (soaked ?tool)) (valid-clean-pair ?loc ?tool))
        :effect (and (not (dusty ?loc)) (not (stained ?loc)))
    )

)